<h1>New Person</h1>
<%= form_for @person do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />

  <%= f.label :age %>
  <%= f.text_field :age %>
  <br />

  <%= f.label :hair_color %>
  <%= f.text_field :hair_color %>
  <br />
 
  <%= f.label :eye_color %>
  <%= f.text_field :eye_color %>
  <br />

  <%= f.label :gender %>
  <%= f.text_field :gender %>
  <br />

  <h6>Needs to be true or false</h6>
  <%= f.label :alive %>
  <%= f.text_field :alive %>
  <br />

  <%= f.submit %>
<% end %>